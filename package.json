{
  "name": "angular2-webpack-express-starter",
  "description": "angular2 webpack express starter",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start:hmr\" \"npm run server\"",
    "start:hmr": "npm run server:dev:hmr",
    "server": "NODE_ENV=development nodemon --use_strict bin/www",
    "server:dev": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "pm2 start process.yml",
    "build:prod": "webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "lint": "tslint \"src/**/*.ts\" -t verbose",
    "clean": "npm run rimraf -- dist",
    "rimraf": "rimraf"
  },
  "dependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0",
    "@angular/http": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/platform-browser-dynamic": "^2.3.0",
    "@angular/router": "^3.3.0",
    "angular-in-memory-web-api": "^0.1.17",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.3.3",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "jade": "~1.11.0",
    "materialize-css": "^0.97.8",
    "morgan": "~1.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.4",
    "serve-favicon": "^2.3.2",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.40",
    "@types/protractor": "^4.0.0",
    "@types/selenium-webdriver": "^2.53.36",
    "@types/source-map": "^0.5.0",
    "@types/systemjs": "^0.19.32",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.0.0",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "^2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "postcss-load-config": "^1.0.0",
    "postcss-loader": "^1.2.0",
    "postcss-smart-import": "^0.6.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.0.2",
    "typescript": "2.1.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^1.0.2"
  }
}
